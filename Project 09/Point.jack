class Point{
    field int xCoordinate;
    field int yCoordinate;
    constructor Point new(int x, int y)
    {
        let xCoordinate = x;
        let yCoordinate = y;
        return this;
    }
    method void dispose()
    {
        do Memory.deAlloc(this);
        return;
    }

    method int getX()
    {
        return xCoordinate;
    }

    method int getY()
    {
        return yCoordinate;
    }
}